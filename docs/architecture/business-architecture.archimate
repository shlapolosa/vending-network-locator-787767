    <!-- Business Processes -->
    <element xsi:type="archimate:BusinessProcess" name="Product Planning Process" id="proc-1">
      <documentation>Quarterly roadmap planning incorporating customer feedback, market analysis, and technical feasibility</documentation>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Feature Development Lifecycle" id="proc-2">
      <documentation>Design → Development → Code Review → Testing → Release; 2-week sprints with continuous deployment</documentation>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Incident Management Process" id="proc-3">
      <documentation>Detection → Triage → Investigation → Resolution → Postmortem; P0: 15min response, P1: 1hr response</documentation>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Security Vulnerability Response" id="proc-4">
      <documentation>Discovery → Assessment → Patch Development → Customer Notification → Deployment; <72hr for critical</documentation>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Enterprise Sales Process" id="proc-5">
      <documentation>Lead → Discovery → Technical Evaluation → POC → Business Case → Negotiation → Close; avg 90-120 days</documentation>
